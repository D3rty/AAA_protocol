<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <File name="file" fileName="log.log">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m %ex%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Logger name="com.d3rty.aaaprotocol" level="all" additivity="false">
      <AppenderRef ref="file"/>
    </Logger>
    <Root level="all">
      <AppenderRef ref="file"/>
    </Root>
  </Loggers>
</Configuration>